<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".editarPerfil">


<TextView
    android:id="@+id/textView5"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="16dp"
    android:layout_marginTop="72dp"
    android:text="Correo electrónico:"
    app:layout_constraintEnd_toStartOf="@+id/editTextEmail"
    app:layout_constraintHorizontal_bias="0.194"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/titulo" />

    <TextView
        android:id="@+id/titulo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="176dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="182dp"
        android:fontFamily="sans-serif-black"
        android:text="CHEMIN"
        android:textColor="@color/cian"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.289"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

<TextView
    android:id="@+id/textView6"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="28dp"
    android:layout_marginTop="60dp"
    android:text="Contraseña:"
    app:layout_constraintEnd_toStartOf="@+id/editTextText5"
    app:layout_constraintHorizontal_bias="0.03"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView5" />

<TextView
    android:id="@+id/textView2"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="16dp"
    android:layout_marginTop="60dp"
    android:text="Nombre completo:"
    app:layout_constraintEnd_toStartOf="@+id/editTextText6"
    app:layout_constraintHorizontal_bias="0.217"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView6" />

<Button
    android:id="@+id/agregarCuenta"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginBottom="108dp"
    android:backgroundTint="@color/cian"
    android:text="Siguiente"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.488"
    app:layout_constraintStart_toStartOf="parent" />

<EditText
    android:id="@+id/editTextEmail"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginEnd="24dp"
    android:ems="10"
    android:inputType="text"
    android:text="Name"
    app:layout_constraintBaseline_toBaselineOf="@+id/textView5"
    app:layout_constraintEnd_toEndOf="parent" />

<EditText
    android:id="@+id/editTextText5"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginEnd="24dp"
    android:ems="10"
    android:inputType="text"
    android:text="Name"
    app:layout_constraintBaseline_toBaselineOf="@+id/textView6"
    app:layout_constraintEnd_toEndOf="parent" />

<EditText
    android:id="@+id/editTextText6"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginEnd="24dp"
    android:ems="10"
    android:inputType="text"
    android:text="Name"
    app:layout_constraintBaseline_toBaselineOf="@+id/textView2"
    app:layout_constraintEnd_toEndOf="parent" />

<EditText
    android:id="@+id/editTextText7"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="26dp"
    android:layout_marginEnd="25dp"
    android:ems="10"
    android:inputType="text"
    android:text="Name"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/editTextText6" />

<TextView
    android:id="@+id/textView7"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="28dp"
    android:layout_marginTop="48dp"
    android:text="Nombre de usuario:"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView2" />

<RadioGroup
    android:id="@+id/radioGroup"
    android:layout_width="264dp"
    android:layout_height="68dp"
    android:layout_marginTop="7dp"
    android:orientation="horizontal"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView7">

    <RadioButton
        android:id="@+id/hombre"
        android:layout_width="wrap_content"
        android:layout_height="33dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="8dp"
        android:text="hombre" />

    <RadioButton
        android:id="@+id/mujer"
        android:layout_width="wrap_content"
        android:layout_height="34dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="12dp"
        android:text="mujer" />

    <RadioButton
        android:id="@+id/otro"
        android:layout_width="70dp"
        android:layout_height="29dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="16dp"
        android:text="otro" />

</RadioGroup>

<TextView
    android:id="@+id/textView9"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="28dp"
    android:layout_marginTop="40dp"
    android:layout_marginEnd="4dp"
    android:text="Seleccionar genero:"
    app:layout_constraintEnd_toStartOf="@+id/radioGroup"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/textView7" />

</androidx.constraintlayout.widget.ConstraintLayout>